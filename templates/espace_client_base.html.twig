<!DOCTYPE html>
<html lang="fr">
<head>
    <title>GOAL{% block title %}{% endblock %}</title>
    <meta charset="UTF-8">
    <link href="/style/style_client.css" rel="stylesheet" type="text/css" >
    <link rel="icon" type="image/jpg" href=/images/photo/goal.jpg>
</head>

<body {% block body_notification %}{% endblock %}>

    <nav id="menu">
            <div id="conteneur_logo">
                <a href="{{path('accueil')}}">
                    <img src="/images/photo/goal_logo.jpg" class="logo">
                </a>
            
            </div>
 <div id="conteneur_liste">
    <ul>

      <li>

        <a href="{{path('logout')}}">
        <img src="/images/icone/sign-out.svg"><span> Déconnexion </span>
        </a>
      </li>
    </ul>
    <ul>
        <li>
            <img src="/images/photo/{{app.user.client.photo}}" ><span>Votre Profile </span>
          <ul>
              <li>
                <a href="#"><img src="/images/icone/padlock.svg" ><span> Mot De Passe </span>
                </a>
              </li>
            <li>
                <a href="#"><img src="/images/icone/pencil.svg" ><span> Nom Et Prénom </span>
                </a>
              </li>
                <li>
                <a href="{{ path('espace_client_modifier_photo')}}"><img src="/images/icone/user.svg" ><span> Photo De Profile </span>
                </a>
            </li>
          </ul>
        </li>
      </ul>
      <ul>
          <li>
            <a href="{{path('espace_client_nos_terrains')}}">
                <img src="/images/icone/soccer-field.svg"><span> Les Terrains </span>
            </a>
          </li>
        </ul>
        <ul>

          <li>
    
            <a href="{{path('espace_client_nos_agences')}}">
                <img src="/images/icone/placeholder.svg"><span> Les Agences </span>
            </a>
          </li>
        </ul>
      <ul>
          <li>
            <a href="#">
                <img src="/images/icone/trophy.svg"><span> Les Tournois </span>
            </a>
          </li>
        </ul>
        <ul>
          <li>
            <a href="{{path('espace_client_mes_reservation')}}">
                <img src="/images/icone/calendar.svg"><span> Réservation </span>
            </a>
          </li>
        </ul>
  </div>
            <div id="conteneur_recherche">
                <div class="searchbar ">
                    <input class="search_input" type="text" name="" placeholder="Chercher...">
                    <a href="#" class="search_icon"><img src="/images/icone/loupe.svg" id="loupe_icone"></a>
                </div>
            </div>
  </nav>
 <section>
    {% block section %}
    {% endblock %}
  </section>
  <footer>
         <div id="social">
            <ul>
              <li>
                    <a href="#"> <img src="/images/icone/facebook.svg" class="social_img"></a>
                </li>
        
                <li>
                    <a href="#"> <img src="/images/icone/twitter.svg" class="social_img"></a>

                </li>
                
                <li>
                    <a href="#"><img src="/images/icone/google-plus.svg" class="social_img"> </a>
                </li>
                    
                <li>
                    <a href="#"><img src="/images/icone/linkedin.svg" class="social_img"></a>
                </li>
               

                <li>
                    <a href="#"><img src="/images/icone/instagram.svg" class="social_img"></a>
                </li>
                
                <li>
                    <a href="#"><img src="/images/icone/pinterest.svg" class="social_img"></a>
                </li>
            </ul>
        </div>
        <div id="Copyright">
           <h4>&copy; 2020 Copyright: Sofiene achour</h4>
        </div>
    </footer>
    <script src="/jquery/jquery-3.2.1.min.js"></script>
    <script src="/js/main.js"></script>
    {% block javascript %}{% endblock %}
</body>
</html>